printf #get screen buffer
$ #goto end of buffer
-1 #go to previous line
,$ #make edits here (at end of buffer)
d\n #delete line
w # write file out
\nq # goto new line and quit ????
\n # ?? new line
| ed #pipe to ed
-s #source
~/test.sh #file to work with.
